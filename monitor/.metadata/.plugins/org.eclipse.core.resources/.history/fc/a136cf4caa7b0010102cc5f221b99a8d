package com.renhenet.web.form;

import com.renhenet.fw.waf.BasePOForm;

public class GroupsForm extends BasePOForm {
	private static final long serialVersionUID = 7453920836891224345L;

	private String name;

	private String memo;

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
