#parse( "/WEB-INF/eportalpages/common/top.vm" )
<body>
<div id="container"> 
<!--site-nav开始-->
#parse( "/WEB-INF/eportalpages/common/site_nav.vm" )
<!--site-nav结束--> 

<!--header开始-->
#parse( "/WEB-INF/eportalpages/common/header.vm" )
<!--header结束--> 

<!--mainnav开始-->
#parse( "/WEB-INF/eportalpages/common/main_nav.vm" )
<!--mainnav结束--> 
  
<!--main开始-->
<div id="main" class="layout">
    <div class="newpage mrg_t15 mrg_b15 fl">当前所在位置：<a href="$!base/index.html" title="首页">首页</a> > FAQ</div>
    <div class="member_nav box1 fl">
      <ul class="menu"><h1>FAQ</h1></ul>
      <ul class="about_list">
        #foreach($biz in $!resourcesList)
        <li #if($typeId == $biz.id) class="on" #end><a href="#if($typeId == $biz.id)javascript:void(0)#else $!base/faqgenre.html?sid=$!vMUtils.encrypt($!biz.id) #end" title="$!biz.value">$!biz.value</a></li>
        #end
      </ul>
    </div>
    <div class="member_bd box1 fr">
      <ul class="menu"><h1>$vMUtils.getResourceValue($typeId)</h1></ul>
      <div class="about_bd">
        <ul class="survey_wz fl">
          #foreach($biz in $!faqgenreList)
          <li><a href="$!base/faqgenre_detail.html?sid=$!vMUtils.encrypt($!biz.id)" title="$!biz.title">$!biz.title</a></li>
          #end
        </ul> 
      </div>
    </div>
  </div>
<!--main结束-->

<!--service开始-->
#parse( "/WEB-INF/eportalpages/common/service.vm" )
<!--service结束-->

<!--footer开始-->
